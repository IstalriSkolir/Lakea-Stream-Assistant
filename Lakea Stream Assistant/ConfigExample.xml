<?xml version="1.0" encoding="utf-8" ?>
<Config>
  <Settings>
    <LogLevel>Warning</LogLevel>
  </Settings>
  <OBS>
    <IP>localhost</IP>
    <Port>{Port}</Port>
    <Password>{OBS_Password}</Password>
  </OBS>
